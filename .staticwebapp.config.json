{
  "routes": [
    {
      "route": "/login.html",
      "serve": "/login.html",
      "statusCode": 200
    },
    {
      "route": "/dashboard.html", 
      "serve": "/dashboard.html",
      "statusCode": 200
    },
    {
      "route": "/factory-3d.js",
      "serve": "/factory-3d.js",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "defaultDocument": "index.html",
  "mimeTypes": {
    ".html": "text/html",
    ".js": "text/javascript",
    ".css": "text/css",
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "no-cache, no-store, must-revalidate"
  }
}